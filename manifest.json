{
  "manifest_version": 3,
  "name": "to/go links",
  "description": "Custom shortcuts to frequently visited resources. Type less, redirect fast.",
  "version": "1.2.0",
  "icons": {
    "16": "./dist/16.png",
    "32": "./dist/32.png",
    "48": "./dist/32.png",
    "128": "./dist/128.png"
  },
  "action": {
    "default_popup": "./dist/popup.html",
    "default_icon": {
      "16": "./dist/16.png",
      "32": "./dist/32.png",
      "48": "./dist/32.png",
      "128": "./dist/128.png"
    }
  },
  "background": {
    "service_worker": "./dist/assets/background.js"
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "storage"
  ]
}
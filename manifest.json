{
  "manifest_version": 3,
  "name": "ToGo",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "./popup.html"
  },
  "background": {
    "service_worker": "./dist/background.js"
  },
  "permissions": [
    "webRequest",
    "webNavigation",
    "tabs",
    "storage",
    "history"
  ],
  "host_permissions": ["<all_urls>"]
}